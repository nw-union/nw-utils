{
  "name": "@nw-union/nw-utils",
  "version": "0.0.6",
  "description": "Our Typescript Utility Library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "bunx tsc -p tsconfig.json",
    "prepublishOnly": "bun run build",
    "check": "biome check --write src"
  },
  "type": "module",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./lib/uuid": {
      "types": "./dist/lib/uuid/index.d.ts",
      "default": "./dist/lib/uuid/index.js"
    },
    "./lib/zod": {
      "types": "./dist/lib/zod/index.d.ts",
      "default": "./dist/lib/zod/index.js"
    },
    "./adapter/auth-mock": {
      "types": "./dist/adapter/auth-mock/index.d.ts",
      "default": "./dist/adapter/auth-mock/index.js"
    },
    "./adapter/auth-cloudflare": {
      "types": "./dist/adapter/auth-cloudflare/index.d.ts",
      "default": "./dist/adapter/auth-cloudflare/index.js"
    },
    "./adapter/log-console": {
      "types": "./dist/adapter/log-console/index.d.ts",
      "default": "./dist/adapter/log-console/index.js"
    },
    "./adapter/log-json": {
      "types": "./dist/adapter/log-json/index.d.ts",
      "default": "./dist/adapter/log-json/index.js"
    }
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nw-union/nw-utils.git"
  },
  "dependencies": {
    "neverthrow": "^8.2.0",
    "ts-pattern": "^5.9.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@types/bun": "^1.3.4"
  }
}
